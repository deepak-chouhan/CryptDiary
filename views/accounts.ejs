                    <input type="password" name="" placeholder="******" id="password">
                </div>
                <div class="show-pass">
                    <input type="checkbox" name="email" id="show-password">
                    <p>Show password</p>
                </div>
                <div class="asset-buttons">
                    <button class="btn btn-full" type="submit" value="accounts" name="asset">Save</button>
